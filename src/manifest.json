{
  "manifest_version": 2,

  "name": "Zooom for Dribbble",
  "description": "Auto load HQ shots on Dribbble.",
  "version": "1.0",
  "content_scripts":[ {
      "js": [ "jquery-2.2.0.min.js","extension.js"],
      "matches": [ "https://dribbble.com/*" ],
      "run_at": "document_start"
   } ],
  "permissions": [ "https://dribbble.com/*" ]
}